{
  "manifest_version": 3,
  "name": "115 Cookie Manager",
  "version": "1.2",
  "description": "Manage cookies for the 115.com",
  "permissions": ["cookies", "storage"],
  "host_permissions": [
    "https://115.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
